{
  "version": "1.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ealush/vast.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "vast",
  "author": "ealush",
  "module": "dist/vast.esm.js",
  "size-limit": [
    {
      "path": "dist/vast.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/vast.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "husky": "^6.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^3.8.0"
  }
}
